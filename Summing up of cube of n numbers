#include<stdio.h>
int main()
{
    int i,n,cube=0;
    int sum=0;
    printf("Enter the numbers till :");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        cube = i*i*i;
        {
            sum += cube;
        }
        printf("The sum of numbers %d is = %d : \n",cube, sum);
    }
    return 0;
}
